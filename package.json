  },
  "dependencies": {
    "ansi-to-html": "~0.3.0",
    "chalk": "~1.1.1",
    "cli-table": "~0.3.1",
    "co": "~4.6.0",
    "commander": "^2.9.0",
    "detect-port": "^1.0.1",
    "macaca-doctor": "~2.0.3",
    "macaca-logo": "~0.1.1",
    "macaca-utils": "^1.0.0",
    "mocha": "^4.0.1",
    "npm-update": "~1.0.2",
    "should": "^8.3.1",
    "webdriver-server": "~1.1.0",
    "xlogger": "~1.0.0"
  },
  "devDependencies": {
    "co-mocha": "*",
    "command-line-test": "^1.0.0",
    "eslint": "^4.14.0",
    "eslint-plugin-mocha": "^4.11.0",
    "istanbul": "*"
